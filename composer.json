{
    "name": "tousleshoraires/contact-bundle",
    "type": "symfony-bundle",
    "description": "Symfony TLHContactBundle",
    "keywords": ["contact form"],
    "homepage": "https://github.com/tousleshoraires/",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Devergnies",
            "email": "julien@tousleshoraires.fr"
        }
    ],
    "require": {
        "php": ">=5.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35|^5.4.3|^6.0|^7.0|^8.0",
        "symfony/class-loader": "*",
        "symfony/framework-bundle": "^3.3|^4.0|^5.0",
        "doctrine/orm": "^2.3",
        "symfony/form": "^3.3|^4.0|^5.0",
        "symfony/validator": "^3.3|^4.0|^5.0",
        "swiftmailer/swiftmailer": "~4.2|~5.0",
        "twig/twig": "^1.0|^2.0"
    },
    "conflict": {},
    "suggest": {},
    "autoload": {
        "psr-4": { "TLH\\ContactBundle\\": "" },
        "exclude-from-classmap": ["Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "support": {
        "docs": "http://www.tousleshoraires.com"
    }
}
